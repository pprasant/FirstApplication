.panel.panel-primary
    .panel-heading.text-center New Courses
    .panel-body
        .row(ng-repeat="course in courses | orderBy: 'published' | limitTo: 8")
            .col-md-3 {{course.published | date:'MMM d'}}
            .col-md-9 
                a(href="/courses/{{course._id}}") {{course.title}}